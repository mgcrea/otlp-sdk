{
  "name": "@mgcrea/otlp-node-sampler",
  "version": "0.2.2",
  "description": "Eslint config for React Native applications",
  "author": "Olivier Louvignes <olivier@mgcrea.io>",
  "repository": "github:mgcrea/eslint-config",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "eslint --config dist/index.js",
    "build": "tsup --entry src/index.ts --format cjs,esm --no-splitting --sourcemap --dts --clean --entry src/bin/cli.ts",
    "prettify": "prettier --write src/ test/",
    "spec": "vitest --run",
    "lint": "eslint src/ test/",
    "prettycheck": "prettier --check src/ test/",
    "typecheck": "tsc --noEmit",
    "test": "npm run lint && npm run prettycheck && npm run typecheck && npm run spec",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/sdk-trace-base": "^1.15.2",
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^18.12.5",
    "@tsconfig/strictest": "^2.0.2",
    "@types/debug": "^4.1.8",
    "@types/eslint": "^8.44.2",
    "@types/node": "^18.17.14",
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "rimraf": "^5.0.1",
    "tsup": "^7.2.0",
    "tsx": "^3.12.8",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3"
  }
}
